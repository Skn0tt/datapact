{
  "name": "datapact-track",
  "private": true,
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz prisma generate && blitz build",
    "start": "blitz start --port=$PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "cd .. && husky install",
    "build:visualiser": "parcel build result_visualiser/entrypoint.tsx --dist-dir ../datapact/javascript"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "node ./db/seeds.js"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-alpha.49",
    "@blitzjs/next": "2.0.0-alpha.49",
    "@blitzjs/rpc": "2.0.0-alpha.49",
    "@chakra-ui/icons": "1.1.7",
    "@chakra-ui/react": "1.8.8",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@prisma/client": "4.0.0",
    "blitz": "2.0.0-alpha.49",
    "chakra-react-select": "3.3.8",
    "chart.js": "3.8.0",
    "date-fns": "2.28.0",
    "final-form": "4.20.7",
    "framer-motion": "6.4.2",
    "mjml": "4.13.0",
    "next": "12.1.6",
    "nodemailer": "6.7.7",
    "parcel": "2.6.2",
    "prisma": "4.0.0",
    "react": "18.2.0",
    "react-chartjs-2": "4.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-syntax-highlighter": "15.5.0",
    "undici": "5.28.3",
    "zod": "3.10.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.6",
    "@types/jest": "28.1.4",
    "@types/node": "17.0.16",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "4.1.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  }
}
